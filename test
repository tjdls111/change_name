import os
from openpyxl import load_workbook


path='C:\\Users\\IDH\\Desktop\\changename\\sample\\'
file_names = os.listdir(path)

load_wb = load_workbook(r"C:\Users\IDH\Desktop\changename\Book1.xlsx", data_only=True)
load_ws = load_wb['Sheet1']
get_cells = load_ws['B2':'B6']
print(get_cells)
for i in range(5):
    if 'zip' in file_names[i]:
        os.rename(path+file_names[i],path+str(get_cells[i][0].value)+'.zip')
    else:
        os.rename(path + file_names[i], path + str(get_cells[i][0].value) + '.jpg')
